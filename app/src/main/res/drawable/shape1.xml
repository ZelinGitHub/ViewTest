<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    android:innerRadius="300dp"
    >
    <corners
        android:radius="5dp"
        />
    <gradient
        android:angle="45"/>
    <padding
        android:left="50dp"
        android:top="50dp"
        android:right="50dp"
        android:bottom="50dp"
        />
